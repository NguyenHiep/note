	function get_lat_long($address=''){
		$address = str_replace(" ", "+", $address);
	
		$json = file_get_contents("http://maps.google.com/maps/api/geocode/json?address=$address&sensor=false");
		$json = json_decode($json);
	
		$lat = $json->{'results'}[0]->{'geometry'}->{'location'}->{'lat'};
		$long = $json->{'results'}[0]->{'geometry'}->{'location'}->{'lng'};
		if($lat != '' && $long != ''){
			return $lat.','.$long;
		}else{
			return '';
		}
	}
	if($supplierinformation['lat'] == '' || $supplierinformation['long'] == ''){
		if($supplierinformation['address'] != ''){
			$addressArr = explode('-',$supplierinformation['address']);
			$address = $addressArr[0];
			foreach($addressArr as $addresss){
				if(strlen($address) < strlen($addresss)){
					$address = $addresss;
				}
			}
		}
		$google_location = $address.' '.$supplierinformation['plz'].' '.$supplierinformation['city'];
		$google_location = str_replace("  ", " ", $google_location);
		$latlong = get_lat_long(utf8_encode($supplierinformation['google_location']));
		if($latlong != ''){
			$supplierhelper -> updatelatlong($supplierinformation['typo3_id'],$latlong);
			$google_location = $supplierinformation['google_location'];
		}else{
			$latlong = get_lat_long($google_location);
			if($latlong != ''){
				$supplierhelper -> updatelatlong($supplierinformation['typo3_id'],$latlong);
			}			
		}
	}
